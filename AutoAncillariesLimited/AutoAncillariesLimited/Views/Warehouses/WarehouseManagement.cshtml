@model AutoAncillariesLimited.Models.Warehouse

@{
  ViewBag.Title = "Warehouse Management";
  Layout = "~/Views/Shared/_MasterLayout.cshtml";
}

<h2>Warehouse Management</h2>
<div class="row">
  <div class="col-lg-6">
    <div class="panel panel-default">
      <div class="panel-heading">
        Category List
        <button class="btn btn-primary pull-right" id="btn-product-insert-form-open">
          <span class="glyphicon glyphicon-plus" style="margin: 0"></span>
        </button>
      </div>
      <div class="panel-body table-responsive">
        <table id="table-warehouses" class="table table-striped table-hover display nowrap">
          <thead>
            <tr>
              <th>ID</th>
              <th>Name</th>
              <th>Address</th>
              <th></th>
            </tr>
          </thead>
        </table>
      </div>
    </div>
  </div>
</div><!--/.row-->

@section scripts
{
  <script src="~/Scripts/pages/warehouse-management.js"></script>
}

